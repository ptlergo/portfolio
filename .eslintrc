{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
        "prettier",
        "angular"
    ],
    "rules": {
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": true, "peerDependencies": true}],
        "prettier/prettier": "error"
    }
}