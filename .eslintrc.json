{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/recommended"
    ],
    "plugins": [
        "import",
        "prettier",
        "angular"
    ],
    "rules": {
        "arrow-body-style": ["error", "always"],
        "one-var": 0,
        "no-undef": 0,
        "prettier/prettier": "error"
    }
}